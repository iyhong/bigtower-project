<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="TreatChart">
 	<select id="treatList" resultType="com.team4.project.government.treatChart.domain.GoTreat">
 		SELECT 
 			GO_TREAT_CODE,
 			GO_CITIZEN_ID,
 			GO_DOCTOR_ID,
 			GO_TREAT_CONTENTS,
 			GO_DISEASE_CODE
 		FROM go_treat
 	</select>
</mapper>
